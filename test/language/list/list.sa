
## List Tests

l = [1, 2, 3]
assert(l.length == 3)
assert(l[0] == 1)
assert(l[1] == 2)
assert(l[2] == 3)

l.append(4)
assert(l.length == 4)
assert(l[3] == 4)

l[0] = 10
assert(l[0] == 10)

l2 = l + [5, 6]
assert(l2.length == 6)
assert(l2[4] == 5)
assert(l2[5] == 6)

## Nested list
l3 = [[1, 2], [3, 4]]
assert(l3[0][0] == 1)
assert(l3[1][1] == 4)

## Iteration
count = 0
for i in l
  count = count + 1
end
assert(count == 4)

## List pop
val = l.pop(0)
assert(val == 10)
assert(l[0] == 2)
assert(l.length == 3)

## String conversion
s = String(l)
assert(s == "[2, 3, 4]")

print("List tests passed!")
