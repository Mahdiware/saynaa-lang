import math

assert(math.abs(-10) == 10)
assert(math.abs(10) == 10)

assert(math.ceil(2.3) == 3)
assert(math.floor(2.8) == 2)
assert(math.round(2.5) == 3)
assert(math.round(2.4) == 2)

assert(math.sqrt(16) == 4)
assert(math.pow(2, 3) == 8)

assert(math.sin(0) == 0)
# simple float check
assert(math.cos(0) == 1)

print("Math module tests passed")
